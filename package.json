{
  "name": "eslint-plugin-todo-with-label",
  "description": "ESLint plugin supports TODO comments with a label in parenthesis",
  "version": "2.0.3",
  "license": "MIT",
  "author": {
    "name": "Piotr Kowalski",
    "email": "piecioshka@gmail.com",
    "url": "https://piecioshka.pl/"
  },
  "scripts": {
    "test": "vitest --run",
    "test:verbose": "DEBUG=eslint:plugins:* vitest --run",
    "coverage": "vitest --coverage --run"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.53.0",
    "eslint-plugin-todo-with-label": "file:./",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:piecioshka/eslint-plugin-todo-with-label.git"
  },
